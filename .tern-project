{
    "ecmaVersion": 6,
    "libs": [
        "browser",
        "ecma6"
    ],
    "loadEagerly": [
        "src/**/*.js"
    ],
    "dontLoad": [
        "node_modules/**",
        "dist/**"
    ],
    "plugins": {
        "node": {},
        "requirejs": {},
        "doc_comment": {
            "fullDocs": true
        }
    }
}
